<!-- Element Definitions -->

	<xs:element name="wass">
	<xs:complexType>
	<xs:sequence>


		<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>


		<xs:element name="login" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="userid" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="password" type="xs:string" minOccurs="0" maxOccurs="1"/>					
					<xs:element name="email" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>


		<xs:element name="saveprefs" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>
					<xs:element name="pref" type="preftype" minOccurs="0" maxOccurs="unbounded"/>
 				</xs:sequence>
			</xs:complexType>	
		</xs:element>		
	

		<xs:element name="getalluserprefs" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>
					<xs:element name="pref" type="preftype" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:complexType>	
		</xs:element>		
		

		<xs:element name="getprefs" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>
					<xs:element name="pref" type="preftype" minOccurs="0" maxOccurs="unbounded"/>
 				</xs:sequence>
			</xs:complexType>	
		</xs:element>		

		
		<xs:element name="validateusers" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
