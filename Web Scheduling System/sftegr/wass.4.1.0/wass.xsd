			<xs:element name="maxapps" type="xs:decimal" minOccurs="0"/>
			<xs:element name="maxper" type="xs:decimal" minOccurs="0"/>			
			<xs:element name="deadline" type="xs:decimal" minOccurs="0"/>
			<xs:element name="candeadline" type="xs:decimal" minOccurs="0"/>
			<xs:element name="opening" type="xs:decimal" minOccurs="0"/>
			<xs:element name="date" type="xs:date" minOccurs="0"/>
			<xs:element name="notify" type="xs:string" minOccurs="0"/>
			<xs:element name="notifyman" type="xs:string" minOccurs="0"/>
			<xs:element name="available" type="xs:string" minOccurs="0"/>
			<xs:element name="makeaccess" type="xs:string" minOccurs="0"/>
			<xs:element name="viewaccess" type="xs:string" minOccurs="0"/>
			<xs:element name="makeulist" type="xs:string" minOccurs="0"/>
