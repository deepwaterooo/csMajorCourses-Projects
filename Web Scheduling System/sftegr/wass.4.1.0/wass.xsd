					<xs:sequence>
						<xs:element name="block" type="blockheaderinfo" minOccurs="0" maxOccurs="1"/>
						<xs:element name="slot" type="slotinfo" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:sequence>
			</xs:complexType>
		</xs:element>			


		<!-- This will return the info for an appointment, given the ID -->
		<xs:element name="getapptinfo" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="apptid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
					<xs:element name="appt" type="apptinfo" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>			

<!-- This will return a list of appointments, with the given filter(s), ordered by date, by time -->
		<!-- Search "appt by" against both "appt with" and "block owner" fields. -->
		<xs:element name="getappts" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="startdate" type="xs:date" minOccurs="1" maxOccurs="1"/>
					<xs:element name="enddate" type="xs:date" minOccurs="0" maxOccurs="1"/>
					<xs:element name="starttime" type="xs:time" minOccurs="0" maxOccurs="1"/>
					<xs:element name="endtime" type="xs:time" minOccurs="0" maxOccurs="1"/>
					<xs:element name="calendarid" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
					<xs:element name="apptby" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="apptwithblock" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="appt" type="apptheaderinfo" minOccurs="0" maxOccurs="unbounded"/>
								<xs:element name="block" type="blockheaderinfo" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>	
					</xs:element>
				</xs:sequence>
			</xs:complexType>
		</xs:element>	


		<!-- Add a new appointment -->
		<xs:element name="addappt" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="appt" type="apptinfo" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>	

		<xs:element name="editappt" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="appt" type="apptinfo" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>	

		<!-- Sync appointment -->
	 	<xs:element name="syncappointment" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="userid" type="xs:string" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="appointmentid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>
		
		
		<!-- Lock appointment - make appointment unavailable or available.  
			"makeavailable" = true will set it to available -->
	 	<xs:element name="lockappointment" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sessionid" type="xs:string" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="appointmentid" type="xs:decimal" minOccurs="1" maxOccurs="1"/>				
					<xs:element name="makeavailable" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:complexType>
		</xs:element>		


	</xs:sequence>
	</xs:complexType>
	</xs:element>

</xs:schema>