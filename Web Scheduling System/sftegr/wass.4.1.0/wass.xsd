<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://wass.princeton.edu/" 
           xmlns="http://wass.princeton.edu/" 
           elementFormDefault="qualified">


<!-- Type Definitions -->
			 
	<xs:complexType name="preftype">
		<xs:sequence>
			<xs:element name="keytag" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="val" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="class" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	
	<xs:simpleType name="weekdaytypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="sunday"/>
			<xs:enumeration value="monday"/>
			<xs:enumeration value="tuesday"/>
			<xs:enumeration value="wednesday"/>
			<xs:enumeration value="thursday"/>
			<xs:enumeration value="friday"/>
			<xs:enumeration value="saturday"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="everytypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="daily"/>
			<xs:enumeration value="dailyweekdays"/>
			<xs:enumeration value="weekly"/>
			<xs:enumeration value="otherweekly"/>
			<xs:enumeration value="monthlyday"/>
			<xs:enumeration value="monthlyweekday"/>
		</xs:restriction>
	</xs:simpleType>

	
	<xs:simpleType name="accesstypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="open"/>
			<xs:enumeration value="limited"/>
			<xs:enumeration value="restricted"/>
			<xs:enumeration value="private"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="recurtypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="once"/>
			<xs:enumeration value="multiple"/>
		</xs:restriction>
	</xs:simpleType>
	
	
	<xs:simpleType name="viewvalues">
		<xs:restriction base="xs:decimal">
			<xs:enumeration value="5"/>
			<xs:enumeration value="10"/>
			<xs:enumeration value="15"/>
			<xs:enumeration value="20"/>
			<xs:enumeration value="30"/>
			<xs:enumeration value="60"/>
		</xs:restriction>
	</xs:simpleType>						
	
	
	<xs:simpleType name="viewlist">
		<xs:list itemType="viewvalues"/>		
	</xs:simpleType>

	
	<xs:simpleType name="datestatusvalues">
		<xs:restriction base="xs:string">
			<xs:enumeration value="noblocks"/>
			<xs:enumeration value="blocksnoappsnoslots"/>
			<xs:enumeration value="blocksnoappsslots"/>
			<xs:enumeration value="blocksappsnoslots"/>
			<xs:enumeration value="blocksappsslots"/>
			<xs:enumeration value="blocksmyappsslots"/>
			<xs:enumeration value="blocksmyappsnoslots"/>
		</xs:restriction>
	</xs:simpleType>						
	
	
	<xs:simpleType name="calendarlisttypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="userid"/>
			<xs:enumeration value="name"/>
			<xs:enumeration value="title"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="callist">
  		<xs:list itemType='xs:decimal'/>
	</xs:simpleType>

	<xs:complexType name="daytypeslist">
		<xs:sequence>
			<xs:element name="date" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="daytype" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
		
	<xs:complexType name="datesavaillist">
		<xs:sequence>
			<xs:element name="date" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="available" type="xs:boolean" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="datestatuslist">
		<xs:sequence>
			<xs:element name="date" type="xs:date" minOccurs="1" maxOccurs="1"/>
			<xs:element name="status" type="datestatusvalues" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
			
			
	<xs:complexType name="periodinfo">	
		<xs:sequence> 
			<xs:element name="starttime" type="xs:time" minOccurs="1"/>
			<xs:element name="endtime" type="xs:time" minOccurs="1"/>
			<xs:element name="dayofweek" type="weekdaytypes" minOccurs="0"/>
			<xs:element name="dayofmonth" type="xs:decimal" minOccurs="0"/>
			<xs:element name="weekofmonth" type="xs:decimal" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
		
	<xs:complexType name="userlist">
		<xs:sequence>
			<xs:element name="userid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="password" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="valid" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
		
	<xs:complexType name="courselist">
		<xs:sequence>
			<xs:element name="courseid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="valid" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	
	<xs:complexType name="appinfo">
		<xs:sequence>
			<xs:element name="appointmentid" type="xs:decimal" minOccurs="0"/>
			<xs:element name="blockid" type="xs:decimal"/>
			<xs:element name="calendarid" type="xs:decimal"/>
			<xs:element name="date" type="xs:date"/>
			<xs:element name="name" type="xs:string"/>
			<xs:element name="email" type="xs:string"/>
			<xs:element name="textemail" type="xs:string"/>
			<xs:element name="phone" type="xs:string"/>
			<xs:element name="userid" type="xs:string"/>
			<xs:element name="starttime" type="xs:time"/>
			<xs:element name="endtime" type="xs:time"/>
			<xs:element name="purpose" type="xs:string" default=""/>
			<xs:element name="remind" type="xs:boolean" default="true"/>
			<xs:element name="notes" type="xs:string" default=""/>
			<xs:element name="uid" type="xs:string" default=""/>
			<xs:element name="whenmade" type="xs:string" default=""/>
			<xs:element name="available" type="xs:boolean" default="true"/>
			<xs:element name="sequence" type="xs:decimal"/>
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:complexType name="blockinfo">
		<xs:sequence>
			<xs:element name="blockid" type="xs:decimal" minOccurs="0"/>
			<xs:element name="periodid" type="xs:decimal" minOccurs="0"/>
			<xs:element name="seriesid" type="xs:decimal" minOccurs="0"/>
