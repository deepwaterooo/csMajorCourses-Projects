				</xs:sequence>
			</xs:complexType>
		</xs:element>


		<xs:element name="validatecourses" minOccurs="0" maxOccurs="1">
			<xs:complexType>
				<xs:sequence>
					<xs:element name="error" type="errorinfo" minOccurs="0" maxOccurs="1"/>
