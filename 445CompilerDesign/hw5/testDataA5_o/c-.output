Grammar

    0 $accept: program $end

    1 program: declaration_list

    2 declaration_list: declaration_list declaration
    3                 | declaration

    4 declaration: var_declaration
    5            | fun_declaration

    6 var_declaration: type_specifier var_decl_list ';'

    7 scoped_var_declaration: STATIC type_specifier var_decl_list ';'
    8                       | type_specifier var_decl_list ';'

    9 var_decl_list: var_decl_list ',' var_decl_initialize
   10              | var_decl_initialize

   11 var_decl_initialize: ID
   12                    | ID '[' NUMCONST ']'
   13                    | ID ':' simple_expression
   14                    | ID '[' NUMCONST ']' ':' simple_expression

   15 type_specifier: INT
   16               | BOOL
   17               | CHAR

   18 fun_declaration: type_specifier ID '(' params ')' statement
   19                | ID '(' params ')' statement

   20 params: param_list
   21       | /* empty */

   22 param_list: param_list ';' type_specifier param_id_list
   23           | type_specifier param_id_list

   24 param_id_list: param_id_list ',' param_id
   25              | param_id

   26 param_id: ID
   27         | ID '[' ']'

   28 statement: ifmatched
   29          | ifunmatched

   30 ifmatched: IF '(' simple_expression ')' ifmatched ELSE ifmatched
   31          | WHILE '(' simple_expression ')' ifmatched
   32          | FOREACH '(' mutable IN simple_expression ')' ifmatched
   33          | expression_stmt

   34 ifunmatched: IF '(' simple_expression ')' ifmatched ELSE ifunmatched
   35            | IF '(' simple_expression ')' statement
   36            | WHILE '(' simple_expression ')' ifunmatched
   37            | FOREACH '(' mutable IN simple_expression ')' ifunmatched

   38 expression_stmt: compound_stmt
   39                | RETURN ';'
   40                | RETURN expression ';'
   41                | BREAK ';'
   42                | expression ';'
   43                | ';'

   44 compound_stmt: '{' local_declarations statement_list '}'

   45 local_declarations: local_declarations scoped_var_declaration
   46                   | /* empty */

   47 statement_list: statement_list statement
   48               | /* empty */

   49 expression: mutable '=' expression
   50           | mutable PASSIGN expression
   51           | mutable MASSIGN expression
   52           | mutable INC
   53           | mutable DEC
   54           | simple_expression

   55 simple_expression: simple_expression OR and_expression
   56                  | and_expression

   57 and_expression: and_expression AND unary_rel_expression
   58               | unary_rel_expression

   59 unary_rel_expression: NOT unary_rel_expression
   60                     | rel_expression

   61 rel_expression: sum_expression '<' sum_expression
   62               | sum_expression '>' sum_expression
   63               | sum_expression LEQ sum_expression
   64               | sum_expression GEQ sum_expression
   65               | sum_expression EQ sum_expression
   66               | sum_expression NEQ sum_expression
   67               | sum_expression

   68 sum_expression: sum_expression '+' term
   69               | sum_expression '-' term
   70               | term

   71 term: term mulop unary_expression
   72     | unary_expression

   73 mulop: '*'
   74      | '/'
   75      | '%'

   76 unary_expression: unaryop unary_expression
   77                 | factor

   78 unaryop: '-'
   79        | '*'

   80 factor: immutable
   81       | mutable

   82 mutable: ID
   83        | ID '[' expression ']'

   84 immutable: '(' expression ')'
   85          | call
   86          | NUMCONST
   87          | CHARCONST
   88          | STRINGCONST
   89          | TRUE
   90          | FALSE

   91 call: ID '(' args ')'

   92 args: arg_list
   93     | /* empty */

   94 arg_list: arg_list ',' expression
   95         | expression


