语法

    0 $accept: program $end

    1 program: declaration_list

    2 declaration_list: declaration_list declaration
    3                 | declaration

    4 declaration: var_declaration
    5            | fun_declaration

    6 var_declaration: type_specifier var_decl_list ';'

    7 scoped_var_declaration: scoped_type_specifier var_decl_list ';'

    8 var_decl_list: var_decl_list ',' var_decl_initialize
    9              | var_decl_initialize

   10 var_decl_initialize: ID
   11                    | ID '[' NUMCONST ']'
   12                    | ID ':' simple_expression
   13                    | ID '[' NUMCONST ']' ':' simple_expression

   14 scoped_type_specifier: STATIC type_specifier
   15                      | type_specifier

   16 type_specifier: INT
   17               | BOOL
   18               | CHAR

   19 fun_declaration: type_specifier ID '(' params ')' statement
   20                | ID '(' params ')' statement

   21 params: param_list
   22       | /* 空 */

   23 param_list: param_list ';' type_specifier param_id_list
   24           | type_specifier param_id_list

   25 param_id_list: param_id_list ',' param_id
   26              | param_id

   27 param_id: ID
   28         | ID '[' ']'

   29 statement: ifmatched
   30          | ifunmatched

   31 ifmatched: IF '(' simple_expression ')' ifmatched ELSE ifmatched
   32          | WHILE '(' simple_expression ')' ifmatched
   33          | FOREACH '(' mutable IN simple_expression ')' ifmatched
   34          | expression_stmt

   35 ifunmatched: IF '(' simple_expression ')' ifmatched ELSE ifunmatched
   36            | IF '(' simple_expression ')' statement
   37            | WHILE '(' simple_expression ')' ifunmatched
   38            | FOREACH '(' mutable IN simple_expression ')' ifunmatched

   39 expression_stmt: compound_stmt
   40                | RETURN ';'
   41                | RETURN expression ';'
   42                | BREAK ';'
   43                | expression ';'
   44                | ';'

   45 compound_stmt: '{' local_declarations statement_list '}'

   46 local_declarations: local_declarations scoped_var_declaration
   47                   | /* 空 */

   48 statement_list: statement_list statement
   49               | /* 空 */

   50 expression: mutable '=' expression
   51           | mutable PASSIGN expression
   52           | mutable MASSIGN expression
   53           | mutable INC
   54           | mutable DEC
   55           | simple_expression

   56 simple_expression: simple_expression OR and_expression
   57                  | and_expression

   58 and_expression: and_expression AND unary_rel_expression
   59               | unary_rel_expression

   60 unary_rel_expression: NOT unary_rel_expression
   61                     | rel_expression

   62 rel_expression: sum_expression '<' sum_expression
   63               | sum_expression '>' sum_expression
   64               | sum_expression LEQ sum_expression
   65               | sum_expression GEQ sum_expression
   66               | sum_expression EQ sum_expression
   67               | sum_expression NEQ sum_expression
   68               | sum_expression

   69 sum_expression: sum_expression '+' term
   70               | sum_expression '-' term
   71               | term

   72 term: term mulop unary_expression
   73     | unary_expression

   74 mulop: '*'
   75      | '/'
   76      | '%'

   77 unary_expression: unaryop unary_expression
   78                 | factor

   79 unaryop: '-'
   80        | '*'

   81 factor: immutable
   82       | mutable

   83 mutable: ID
   84        | ID '[' expression ']'

   85 immutable: '(' expression ')'
   86          | call
   87          | NUMCONST
   88          | CHARCONST
   89          | STRINGCONST
   90          | TRUE
   91          | FALSE

   92 call: ID '(' args ')'

   93 args: arg_list
   94     | /* 空 */

   95 arg_list: expression ',' arg_list
   96         | expression


