"{"                     {return '{';}
"}"                     {return '}';}
"("                     {return '(';}
")"                     {return ')';}
","                     {return ',';}
":"                     {return ':';}
";"                     {return ';';}
"+"                     {return '+';}
"-"                     {return '-';}
"*"                     {return '*';}
"/"                     {return '/';}
"%"                     {return '%';}
"<"                     {return '<';}
">"                     {return '>';}
"="                     {return '=';}

main() {
	// lex through the input:
	yylex();
}



